{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": "airbnb",
  "rules": {
    "semi": [
      0
    ],
    "react/jsx-filename-extension": [
      0
    ],
    "indent": [
      1,
      2,
      {
        "SwitchCase": 2
      }
    ],
    "no-tabs": 0
  }
}
